def chocolate_number(total, unitprice):
    chocolate_number = int(total/unitprice)
    change = float(total % unitprice)
    return chocolate_number, change


total_money = float(input("total_money:"))
chocolate_unit_price = float(input("chocolate_unit_price:"))
num = chocolate_number(total_money, chocolate_unit_price)
print("chocolate number:", num[0], "change:", num[1])
"""
    Example:
        input part:
            total_money:10
            chocolate_unit_price:2
        output part:
            chocolate number:5
            change:0
"""

